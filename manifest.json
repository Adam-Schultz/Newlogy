{
    "manifest_version": 2,
    "name": "Newlogy",
    "description": "Remove unnecessary components from the Schoology interface",
    "version": "1.0",
    "content_scripts": [{
        "matches": ["https://*.schoology.com/*", "https://schoology.com/*"],
        "js": ["clean.js"],
        "css": ["clean.css"]
    }],
    "icons": {
        "32": "icons/32.png",
        "64": "icons/64.png",
        "128": "icons/128.png",
        "256": "icons/256.png"
    },
    "permissions": [
        "activeTab"
    ]
}
